{
  "profiles": {
    "http": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONNECTION_STRING": "Host=localhost;Database=WildApples;Username=postgres;Password=postgres"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5254"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONNECTION_STRING": "Host=localhost;Database=WildApples;Username=postgres;Password=postgres"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "https://localhost:7175;http://localhost:5254"
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "CONNECTION_STRING": "Host=localhost;Database=WildApples;Username=postgres;Password=postgres"
      }
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/swagger",
      "environmentVariables": {
        "ASPNETCORE_HTTPS_PORTS": "8081",
        "ASPNETCORE_HTTP_PORTS": "8080",
        "CONNECTION_STRING": "Host=localhost;Database=WildApples;Username=postgres;Password=postgres"
      },
      "publishAllPorts": true,
      "useSSL": true
    }
  },

  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:33440",
      "sslPort": 44310
    }
  }
}